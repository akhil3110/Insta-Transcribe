{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext","es5","es2015.promise"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/api/upload/route.js", "app/api/transcribe/route.js", "app/(routes)/(video-transcribe-page)/[filename]/page.tsx", "app/(routes)/(video-transcribe-page)/[filename]/_components/result-column.tsx", "lib/to-srt.js"],
  "exclude": ["node_modules"]
}
